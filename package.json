{
  "name": "master-hook",
  "version": "1.4.1",
  "description": "Easy connect to Redux by hooks with caching",
  "author": "opium.pro",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon -e ts,tsx -w ./src -x npm run dist",
    "build": "rm -fr ./dist && tsc",
    "publish": "npm run build && npm publish",
    "dist": "npm run build && rsync -a ./dist/ ../master-hook-playground/node_modules/master-hook/dist/"
  },
  "dependencies": {
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-redux": "^7.1.16",
    "react": "^17.0.1",
    "nodemon": "^2.0.12",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opium-pro/master-hook.git"
  },
  "keywords": [
    "redux",
    "hooks",
    "react",
    "store",
    "local storage",
    "cache",
    "react-redux",
    "easy redux"
  ],
  "contributors": [
