{
  "name": "master-hook",
  "version": "2.0.2",
  "description": "Easy connect to Redux by hooks with caching",
  "author": "opium.pro",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "npm run watch",
    "watch": "nodemon -e ts,tsx -w ./src -x npm run build",
    "build": "tsc",
    "release": "npm run clear && npm run build && npm publish",
    "clear": "rm -fr ./dist"
  },
  "dependencies": {
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-redux": "^7.1.16",
    "nodemon": "^2.0.12",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opium-pro/master-hook.git"
  },
  "keywords": [
    "redux",
    "hooks",
    "react",
    "store",
    "local storage",
    "cache",
    "react-redux",
    "easy redux"
  ],
  "contributors": [
    {
      "name": "Rodenstern",
      "url": "https://github.com/Rodenstern"
    },
    {
      "name": "Denis Elianovsky",
      "url": "https://opium.pro/contacts/denis"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opium-pro/master-hook/issues"
  }
}
